<div class="container" style="margin-top: 50px;">
    <table class="table table-dark table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">CompanyName</th>
      <th scope="col">Email</th>
      <th scope="col">Mobile</th>
      <th scope="col">Password</th>
      <th scope="col">City</th>
      <th scope="col">Status</th>
         <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
      {{#each shops}}
    <tr>
      <th scope="col">#</th>
      <th scope="col">{{this.ShopName}}</th>
      <th scope="col">{{this.email}}</th>
      <th scope="col">{{this.mobile}}</th>
      <th scope="col">{{this.password}}</th>
      <th scope="col">{{this.city}}</th>
      <th scope="col">{{this.status}}</th>
       <th scope="col"> <a href="/sadmin/approve/{{this.id}}" class="btn btn-primary">approve</a></th>
    </tr>
    {{/each}}
  </tbody>
</table>
</div>